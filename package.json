{
  "name": "bitdao-os",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "ci:app-template": "turbo e2e:test:app-template",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:api": "turbo run dev:api",
    "dev:app-template": "turbo run dev:app-template",
    "dev:app-exp": "turbo run dev:app-exp",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@bitdao/eslint-config-next": "workspace:*",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/node": "18.11.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "packageManager": "pnpm@7.18.2"
}
